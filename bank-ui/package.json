{
  "name": "@midnight-bank/bank-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "http-server --port 8080 ./dist --proxy 'http://127.0.0.1:8080?' --history-api-fallback",
    "dev": "npm run dev:prepare && vite --host --force",
    "dev:local": "npm run config:local && npm run dev",
    "dev:testnet": "npm run config:testnet && npm run dev",
    "dev:prepare": "rimraf public/keys public/zkir || true && mkdir -p public/keys public/zkir && cp -f /Users/norman/Development/midnight/midnight-bank/bank-contract/src/managed/bank/keys/* public/keys/ 2>/dev/null || true && cp -f /Users/norman/Development/midnight/midnight-bank/bank-contract/src/managed/bank/zkir/* public/zkir/ 2>/dev/null || true",
    "build": "vite build && npm run copy-bank-keys",
    "build:local": "npm run config:local && npm run build",
    "build:testnet": "npm run config:testnet && npm run build",
    "config:local": "cp public/config.local.json public/config.json",
    "config:testnet": "cp public/config.testnet.json public/config.json",
    "copy-bank-keys": "mkdir -p ./dist/keys ./dist/zkir && cp -r ../bank-contract/src/managed/bank/keys/* ./dist/keys || true && cp -r ../bank-contract/src/managed/bank/zkir/* ./dist/zkir || true",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.3",
    "@midnight-bank/bank-api": "file:../bank-api",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@tanstack/react-query": "^5.85.0",
    "pino": "^8.16.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.3.1",
    "http-server": "^14.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.4.1"
  }
}
